[%- WRAPPER "layout.tt" title=l("URL Information") -%]
    [%- WRAPPER "components/box.tt" header=l("URL Information") -%]
        <table class="details">
            <tr>
                <th>
                    [%- l('URL:') -%]
                </th>
                <td>
                    [%- entity_link(url) -%]
                </td>
            </tr>
            <tr>
                <th>
                    [%- l('Description:') -%]
                </th>
                <td>
                    [%- (url.desc || l("This URL has no description")) | html -%]
                </td>
            </tr>
            <tr>
                <th>
                    [%- doc_link("MusicBrainzIdentifier", l("MBID")) -%]:
                </th>
                <td>
                    [%- url.mbid -%]
                </td>
            </tr>
        </table>
    [%- END -%]
    [%- INCLUDE "components/relationships.tt" showall=1 source=url -%]
[%- END -%]
