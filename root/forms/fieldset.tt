<fieldset
    [%- IF id -%]
        id="[%- id -%]"
    [%- END %]
    [%- IF class -%]
        class="[%- class -%]"
    [%- END %]
    [%- IF es -%]
        id="es-form"
    [%- ELSIF re -%]
        id="es-events"
    [%- END -%]>
    <legend>[%- legend -%]</legend>
    [%- content -%]
</fieldset>
